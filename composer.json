{
    "name": "bewarhizirvan/laravelform",
    "description": "A Form generator for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Bewar Hizirvan",
            "email": "bewar@hizirvan.email",
            "homepage": "hizirvan.app"
        }
    ],
    "homepage": "https://github.com/bewarhizirvan/laravel-form",
    "keywords": ["Laravel", "LaravelForm"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BewarHizirvan\\LaravelForm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BewarHizirvan\\LaravelForm\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BewarHizirvan\\LaravelForm\\LaravelFormServiceProvider"
            ],
            "aliases": {
                "LaravelForm": "BewarHizirvan\\LaravelForm\\Facades\\LaravelForm"
            }
        }
    }
}
